{
    "name": "aft/datamodel",
    "description": "AFT data model management",
    "type": "library",
    "version": "1.0",
    "authors": [
        {
            "name": "David Richards",
            "email": "drichard@aft.org"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">=7.2",
        "phpoffice/phpspreadsheet": "^1.14",
        "twig/twig": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Aft\\DataModel\\": "src/",
            "App\\Models\\": "output/Models/",
            "App\\Http\\Controllers\\": "output/Controllers/",
            "App\\Http\\Requests\\": "output/Requests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Aft\\DataModel\\Providers\\DataModelServiceProvider"
            ]
        }
    }
}
